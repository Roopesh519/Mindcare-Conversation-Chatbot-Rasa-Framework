{
    "builds": [
      {
        "src": "Mindcare/MindCare/manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Mindcare/MindCare/manage.py"
      }
    ],
    "hooks": {
      "build": "python custom-setup.py"
    }
  }
  